# WalletConnect Project ID
# Get from https://cloud.walletconnect.com/
VITE_WALLETCONNECT_PROJECT_ID=your_project_id_here

# Arc Network RPC URLs
VITE_ARC_TESTNET_RPC=https://rpc-testnet.arc.network
VITE_ARC_MAINNET_RPC=https://rpc.arc.network

# Contract Addresses (Arc Testnet)
VITE_PAYMENT_ROUTER_ADDRESS=0xAeeAe0B6dBD6EF6eFE2fec07a720c65AeFb2492A
VITE_FX_ROUTER_ADDRESS=0x1234567890123456789012345678901234567890
VITE_USDC_ADDRESS=0xA0b86991c6218b36c1d19D4a2e9Eb0cE3606eB48

# Explorer URLs
VITE_ARC_TESTNET_EXPLORER=https://explorer-testnet.arc.network
VITE_ARC_MAINNET_EXPLORER=https://explorer.arc.network

# API Keys (Optional)
# VITE_CHAINLINK_API_KEY=your_chainlink_api_key
# VITE_ANALYTICS_KEY=your_analytics_key

# Feature Flags
VITE_ENABLE_MOCK_DATA=true
VITE_ENABLE_DEBUG=true